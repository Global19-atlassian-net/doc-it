  <refentry id='function.dom-domnamednodemap-item'>
   <refnamediv>
    <refname>DOMNamedNodeMap->item</refname>
    <refpurpose>Recupera un nodo tramite il suo indice</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descrizione</title>
    <methodsynopsis>
     <type>object</type><methodname>DOMNamedNodeMap->item</methodname>
     <methodparam><type>int</type><parameter>index</parameter></methodparam>
    </methodsynopsis>
    <para>
     Recupera un nodo tramite l'indice <parameter>index</parameter> all'interno dell'oggetto
     DOMNamedNodeMap.
    </para>
   </refsect1>
  </refentry>

